server{

    server_name kleikoks.fun;
    listen 80;


    location /media {
        alias /home/kleikoks/projects/visatut/src/media;
    }

    location /static {
        alias /home/kleikoks/projects/visatut/src/static_root;
    }

    location / {
        uwsgi_pass unix:/home/kleikoks/projects/visatut/src/config/visatut.sock;
        include /etc/nginx/uwsgi_params;
    }

}
